{
  "name": "vite-react-express-boilerplate",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "nodemon src/server.js",
    "build": "npm run server:build && npm run client:build",
    "client:build": "vite build",
    "server:build": "babel --verbose --out-dir dist --only 'src/server.js,src/api/,src/lib' src",
    "test": "vitest",
    "lint": "eslint --ext .js,.jsx .",
    "coverage": "vitest run --coverage",
    "cypress": "cypress open",
    "import": "npm run import-journey && npm run import-station",
    "import-journey": "node src/lib/importJourneyData.js",
    "import-station": "node src/lib/importStationData.js",
    "dedup": "node src/lib/deduplicate.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/coverage-c8": "^0.31.0",
    "@vitest/ui": "^0.31.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.1",
    "cypress": "^12.12.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-proxy-middleware": "^2.0.1",
    "jsdom": "^22.0.0",
    "nodemon": "^2.0.13",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "stylelint": "^15.6.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "vite": "^2.5.10",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@mui/x-date-pickers": "^6.3.0",
    "axios": "^1.3.6",
    "chart.js": "^4.3.0",
    "console-log-colors": "^0.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fast-csv": "^4.3.6",
    "leaflet": "^1.9.3",
    "mongodb": "^5.4.0",
    "mongoose": "^7.0.4",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.10.0",
    "react-spinners": "^0.13.8",
    "supertest": "^6.3.3"
  }
}
